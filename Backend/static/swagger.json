  "paths": {
    "/routine": {
      "get": {
        "tags": ["Routine"],
        "summary": "Get routine",
        "parameters": [
          {
            "name": "trainerEmail",
            "in": "query",
            "description": "Trainer email",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "example": {
                  "routine": {
                    "id": 1,
                    "name": "Example Routine",
                    "description": "Routine description"
                  }
                }
              }
            }
          }
        }
      },
      "post": {
        "tags": ["Routine"],
        "summary": "Create routine",
        "parameters": [
          {
            "name": "Routine",
            "in": "body",
            "description": "Routine details",
            "required": true,
            "schema": {
              "$ref": "#/definitions/Routine"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "example": "Successfully created routine"
              }
            }
          }
        }
      },
      "delete": {
        "tags": ["Routine"],
        "summary": "Delete routine",
        "parameters": [
          {
            "name": "routineId",
            "in": "query",
            "description": "Routine ID",
            "required": true,
            "schema": {
              "type": "integereger"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "example": "Successfully deleted routine"
              }
            }
          }
        }
      }
    },
    "/routine-exercise": {
      "post": {
        "tags": ["Routine"],
        "summary": "Add routine exercise",

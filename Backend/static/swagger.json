      "get": {
        "tags": ["Diet"],
        "summary": "Get Diets",
        "parameters": [
          {
            "name": "dietType",
            "in": "query",
            "description": "Type of diet",
            "required": false,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/Diet"
              }
            }
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      },
      "post": {
        "tags": ["Diet"],
        "summary": "Create or Edit Diet",
        "consumes": ["application/json"],
        "produces": ["application/json"],
        "parameters": [
          {
            "name": "dietId",
            "in": "body",
            "description": "ID of the diet (for editing, optional)",
            "required": false,
            "type": "integer"
          },
          {
            "name": "trainerEmail",
            "in": "body",
            "description": "Trainer's email",
            "required": true,
            "type": "string"
          },
          {
            "name": "dietType",
            "in": "body",
            "description": "Type of diet",
            "required": true,
            "type": "string"
          },
          {
            "name": "breakfast",
            "in": "body",
            "description": "Breakfast details",
            "required": true,
            "type": "string"
          },
          {
            "name": "lunch",
            "in": "body",
            "description": "Lunch details",
            "required": true,
            "type": "string"
          },
          {
            "name": "dinner",
            "in": "body",
            "description": "Dinner details",
            "required": true,
            "type": "string"
          },
          {
            "name": "instruction",
            "in": "body",
            "description": "Diet instructions",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "Diet created/updated successfully"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      },
      "delete": {
        "tags": ["Diet"],
        "summary": "Delete Diet",
        "consumes": ["application/json"],
        "produces": ["application/json"],
        "parameters": [
          {
            "name": "dietId",
            "in": "body",
            "description": "ID of the diet to delete",
            "required": true,
            "type": "integer"
          },
          {
            "name": "trainerEmail",
            "in": "body",
            "description": "Trainer's email",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "Diet deleted successfully"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      }
    },
    "/assign-diet": {
      "post": {
        "tags": ["Diet"],
        "summary": "Assign Diet to User",
        "consumes": ["application/json"],
        "produces": ["application/json"],
        "parameters": [
          {
            "name": "dietName",
            "in": "body",
            "description": "Name of the diet to assign",
            "required": true,
            "type": "string"
          },
          {
            "name": "trainerEmail",
            "in": "body",
            "description": "Trainer's email",
            "required": true,
            "type": "string"
          },
          {
            "name": "clientEmail",
            "in": "body",
            "description": "Client's email",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "Diet assigned to user successfully"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      }
    }

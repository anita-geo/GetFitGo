    "/login": {
      "post": {
        "tags": [
          "Register/Login"
        ],
        "description": "Returns if the login is correct",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "access data",
            "in": "body",

      "post": {
        "tags": ["Register/Login"],
        "summary": "Register a Trainer",
        "consumes": ["application/json"],
        "produces": ["application/json"],
        "parameters": [
          {
            "name": "Trainer",
            "in": "body",
            "description": "Trainer details",
            "required": true,
            "schema": {
              "$ref": "#/definitions/Trainer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Trainer created successfully"
          },
          "400": {
            "description": "Bad request"
          },
          "500": {
            "description": "Failed to create trainer"
          }
        }
      }
    },
    "/register-client": {
      "post": {
        "tags": ["Register/Login"],
        "summary": "Register a Client",

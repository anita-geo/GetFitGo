{
  "swagger": "2.0",
  "info": {
    "title": "GetFitGo API",
    "version": "1.0.0"
  },
  "tags": [
    {
      "name": "Register/Login",
      "description": "Login and register APIs"
    },
    {
      "name": "Routine",
      "description": "Routine APIs"
    },
    {
      "name": "Exercise",
      "description": "Exercise APIs"
    },
    {
